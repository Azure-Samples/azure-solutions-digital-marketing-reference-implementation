  http://go.microsoft.com/fwlink/?LinkId=301880
  -->
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <appSettings file="developerSettings.config">
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- Azure Active Directory Settings-->
    <add key="ida:ClientId" value="" />
    <add key="ida:TenantId" value="" />
    <add key="ida:Domain" value="" />
    <add key="ida:AADInstance" value="https://login.microsoftonline.com/" />
    <add key="ida:PostLogoutRedirectUri" value="https://localhost:44300/" />
    <!--
    ######*******IMPORTANT*******######
    Add storage account name and key for development
    At deployment time, these values (at least the keys) should be set in Azure 
    portal / script to avoid storing keys in web.config
    Check for, or create a DeveloperSettings.config file to manage these 
    keys outside the web.config
    -->
    <!-- The name of the azure storage account (not URL) and access key-->
    <add key="azureStorageAccountName" value="" />
    <add key="azureStorageAccountKey" value="" />
    <!--Container names in Azure Blob storage. 
    These can be any valid name but must match the container names used during setup-->
    <!--<add key="mediaStorageVideoContainerName" value="videos" />-->
    <add key="mediaStorageImageContainerName" value="images" />
    <!-- Timeout for the blob storage key used to do media uploads to Blob storage-->
    <add key="mediaStorageUploadPolicyTimeout" value="10" />
    <!--The name, not URL, of the Doc DB Server and the access key-->
    <add key="azureDocumentDBServer" value="" />
    <add key="azureDocumentDBKey" value="" />
    <!--The name of the doc db database to use and the collection names
    note that these names just need to match what is created in Doc DB
    and can even be the same name if desired. They are split here to provide options for scaling independently-->
    <add key="azureDocDBDatabaseName" value="azurekit" />
    <add key="azureDocumentDBContentCollection" value="SiteContent" />
    <!--<add key="azureDocumentDBMediaCollection" value="MediaLibraries" />-->
    <add key="azureStorageCDN" value="" />
    <add key="redisCacheConnection" value="" />
  </appSettings>

  <!-- Copy the connection string for your notification hub from the azure portal-->
  <connectionStrings>
    <add name="MS_NotificationHubConnectionString" connectionString="Endpoint=sb://YOUR_SB_NH_Namespace.servicebus.windows.net/;SharedAccessKeyName=DefaultFullSharedAccessSignature;SharedAccessKey=YOUR_ACCESS_KEY" />
  </connectionStrings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
